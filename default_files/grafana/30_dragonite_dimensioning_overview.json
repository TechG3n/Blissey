          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  count(distinct(a.name)) as 'areas',\r\n  sum(round(2*b.stops/$numQuest,2)) as '#quest_req',\r\n  ceiling(2*sum(b.stops)/$numQuest)-cast(sum(a.quest_mode_workers) as SIGNED) as 'quest_diff',\r\n  sum( round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2) ) as '#mon_req',\r\n  round(sum( round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2) )- sum( CAST(a.pokemon_mode_workers AS SIGNED) ) ,0) as 'mon_diff',\r\n  sum(pokemon_mode_workers) as 'current',\r\n  ceiling(greatest(sum(round(2*b.stops/$numQuest,2)),sum( round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2) )) ) as 'final_req',\r\n  ceiling(greatest(sum(round(2*b.stops/$numQuest,2)),sum( round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2) )) ) - sum(a.pokemon_mode_workers) as 'final_diff'\r\nFROM ${Dragonite:text}.area a\r\nLEFT JOIN ${Blissey:text}.stats_quest_area b ON a.name COLLATE utf8mb4_general_ci = area and b.datetime = concat(date(now() - interval 15 minute),' ', SEC_TO_TIME((TIME_TO_SEC(time(now() - interval 15 minute)) DIV 900) * 900)) and b.rpl = 15\r\nWHERE a.pokemon_mode_workers>0\r\n",
