          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  a.name,\r\n  round(2*sum(b.stops)/$numQuest,2) as '#quest_req',\r\n  (ceiling(2 * sum(b.stops) / $numQuest)) - IF(a.quest_mode_workers = 0, a.pokemon_mode_workers, a.quest_mode_workers) AS 'quest_diff',\r\n  round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2) as '#mon_req',\r\n  CAST(ceiling( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ) AS SIGNED) - CAST(a.pokemon_mode_workers AS SIGNED) as 'mon_diff',\r\n  sum(pokemon_mode_workers) as 'current',\r\n  case when a.pokemon_mode_workers>0 then ceiling(greatest(round(2*sum(ifnull(b.stops,0))/$numQuest,2),round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2)) ) end as 'final_req',\r\n  case when a.pokemon_mode_workers>0 then ceiling((ceiling(greatest(round(2*sum(ifnull(b.stops,0))/$numQuest,2),round( (($locTime) * ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / ($rtt*60) ,2)) ))-a.pokemon_mode_workers) end as 'final_diff'\r\nFROM ${Dragonite:text}.area a\r\nLEFT JOIN ${Blissey:text}.stats_quest_area b ON a.name COLLATE utf8mb4_general_ci = area and b.datetime = concat(date(now() - interval 15 minute),' ', SEC_TO_TIME((TIME_TO_SEC(time(now() - interval 15 minute)) DIV 900) * 900)) and b.rpl =15\r\n\r\nGROUP BY a.name\r\nORDER BY a.name",
