          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  count(distinct(a.name)) as 'areas',\r\n  sum(a.pokemon_mode_workers) as 'mon worker',\r\n  sum(ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\"))))  as 'mon length',\r\n  sum(ROUND((LENGTH(a.pokemon_mode_route)-LENGTH(REPLACE(a.pokemon_mode_route, \",\", \"\"))+1) / (1 * LENGTH(\",\")))) / sum(a.pokemon_mode_workers) as '#mon loc/worker',\r\n  sum(b.stops) as '#stops',\r\n  sum(b.stops) / sum(a.pokemon_mode_workers) as '#quest/worker'\r\n\r\nFROM ${Dragonite:text}.area a\r\nLEFT JOIN ${Blissey:text}.stats_quest_area b ON a.name COLLATE utf8mb4_general_ci = area and b.datetime = concat(date(now() - interval 15 minute),' ', SEC_TO_TIME((TIME_TO_SEC(time(now() - interval 15 minute)) DIV 900) * 900)) and b.rpl =15\r\nWHERE a.pokemon_mode_workers>0",
